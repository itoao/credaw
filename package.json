{
  "name": "credaw",
  "type": "module",
  "version": "1.0.4",
  "description": "CLI tool for easy AWS credential management.",
  "author": "aoito",
  "license": "MIT",
  "keywords": [
    "AWS",
    "CLI",
    "nodejs",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/run.mjs",
      "require": "./dist/run.cjs"
    }
  },
  "main": "./dist/run.cjs",
  "bin": {
    "credaw": "./dist/run.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "jiti src/run.ts",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "fs-extra": "^11.2.0",
    "inquirer": "^10.1.8",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "eslint": "^9.9.0",
    "jiti": "^1.21.6"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.5"
  }
}
